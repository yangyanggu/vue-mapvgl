{
  "plugins": ["vue"],
  "extends": "elemefe",
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "mocha": true
  },
  "globals": {
      "VueMapvgl": true,
      "particlesJS": true,
      "expect": true,
      "sinon": true,
      "BMapGL": true
  }
}
